program Project1;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {button12},
  Unit2 in 'Unit2.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Tbutton12, button12);
  Application.CreateForm(TForm2, Form2);
  Application.Run;
end.
